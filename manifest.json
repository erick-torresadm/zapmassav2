{
  "manifest_version": 3,
  "name": "WhatsApp Contact Saver Pro",
  "version": "2.0",
  "description": "Salve seus contatos do WhatsApp de forma eficiente",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://web.whatsapp.com/*",
    "https://script.google.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}